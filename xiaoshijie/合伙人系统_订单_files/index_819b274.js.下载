Vue.http.options.emulateJSON=!0;var vm=new Vue({el:"#app",data:function(){return{orders:[],orderSum:"",incomeSum:"",paySum:"",orderNo:vm_orderNo||"",status:"",startTime:"",endTime:"",wp:"",isEnd:!1,loading:!1,statusNO:!1,sliderNo:!1,statusName:"所有状态"}},mounted:function(){this.fetchData()},methods:{openSilider:function(){this.sliderNo=!0},openStatus:function(){this.statusNO=!this.statusNO},statusClick:function(t){this.status=t,0===t&&(this.statusName="所有状态"),1===t&&(this.statusName="已付款"),3===t&&(this.statusName="已结算"),4===t&&(this.statusName="已失效"),this.wp="",this.isEnd=!1,this.fetchData()},resetDate:function(){this.startTime="",this.endTime=""},dateClick:function(){this.wp="",this.isEnd=!1,this.fetchData()},fetchData:function(){if(!this.loading&&!this.isEnd){this.loading=!0;var t={orderNo:this.orderNo,status:this.status,startTime:this.startTime,endTime:this.endTime,wp:this.wp};this.$http.post("/h5/fx/order/data",t).then(function(t){var s=JSON.parse(t.data);1001===s.status.code?(this.orders=(this.status>=0||""!==this.startTime||""!==this.endTime)&&""===this.wp?s.result.items:this.orders.concat(s.result.items),this.orderSum=s.result.orderSum,this.incomeSum=s.result.incomeSum,this.paySum=s.result.paySum,this.wp=s.result.wp,this.isEnd=s.result.isEnd):alert(s.status.msg),this.sliderNo=!1,this.loading=!1}.bind(this))}}}});