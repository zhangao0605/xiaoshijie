Vue.http.options.emulateJSON=!0;var vm=new Vue({el:"#app",data:function(){return{agents:[],checks:[],checkIndex:-1,agentName:vm_agentName||"",total:"",page:0,wp:"",isEnd:!1,loading:!1}},created:function(){this.fetchData()},mounted:function(){},methods:{initCheck:function(){for(var t=0;t<this.agents.length;t++){{this.agents[t]}this.checks.push(!1)}},expand:function(t){this.checks.splice(t,1,!this.checks[t]),this.checkIndex=this.checks[t]?t:-1},fetchData:function(){if(!this.loading&&!this.isEnd){this.loading=!0;var t={agentName:this.agentName,wp:this.wp};this.$http.get("/h5/fx/agent/data",{params:t}).then(function(t){var e=JSON.parse(t.data);1001===e.status.code?(this.agents=e.result.agents,this.total=e.result.total,this.page=e.result.page,this.wp=e.result.wp,this.isEnd=e.result.isEnd,this.initCheck()):alert(e.status.msg),this.loading=!1}.bind(this))}}}});