Vue.http.options.emulateJSON=!0;var vm=new Vue({el:"#app",data:function(){return{lists:[],wp:"",isEnd:!1,loading:!1}},mounted:function(){this.fetchData()},methods:{fetchData:function(){if(!this.loading&&!this.isEnd){this.loading=!0;var t={wp:this.wp};this.$http.get("/h5/fx/fee/detailapply",{params:t}).then(function(t){var s=JSON.parse(t.data);1001===s.status.code?(this.lists=this.lists.concat(s.result.bills),this.wp=s.result.wp,this.isEnd=s.result.isEnd):alert(s.status.msg),this.loading=!1}.bind(this))}}}});